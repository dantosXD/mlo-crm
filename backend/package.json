{
  "name": "mlo-dashboard-backend",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/bootstrap.ts",
    "worker": "tsx src/worker.ts",
    "build": "tsc",
    "start": "node dist/bootstrap.js",
    "lint": "eslint . --ext ts --report-unused-disable-directives",
    "lint:ci": "eslint . --ext ts --report-unused-disable-directives --max-warnings=12",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "typecheck": "tsc --noEmit",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:migrate:deploy": "prisma migrate deploy",
    "db:seed": "tsx prisma/seed.ts",
    "db:validate-integrity": "tsx scripts/validate-postgres-integrity.ts",
    "db:studio": "prisma studio",
    "migrate:sqlite-to-postgres": "tsx scripts/migrate-sqlite-to-postgres.ts"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.981.0",
    "@aws-sdk/s3-request-presigner": "^3.981.0",
    "@sentry/node": "^8.44.0",
    "@prisma/client": "^5.7.1",
    "@types/cookie-parser": "^1.4.10",
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^8.2.1",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "node-fetch": "^2.7.0",
    "rate-limit-redis": "^4.2.2",
    "redis": "^4.7.0",
    "socket.io": "^4.6.1",
    "uuid": "^9.0.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/express-rate-limit": "^5.1.3",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.10.6",
    "@types/pg": "^8.11.10",
    "@types/supertest": "^6.0.3",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "better-sqlite3": "^12.6.2",
    "eslint": "^8.56.0",
    "pg": "^8.13.1",
    "prisma": "^5.7.1",
    "supertest": "^7.1.4",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vitest": "^1.1.1"
  }
}
